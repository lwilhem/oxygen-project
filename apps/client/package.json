{
	"name": "@oxygen/client",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"format": "prettier --write --ignore-path ../../.gitignore \"**/*.{svelte,js,ts,html,json,cjs}\"",
		"lint": "eslint --ignore-path ../../.gitignore \"**/*.{ts,svelte}\"",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"build": "vite build"
	},
	"dependencies": {
		"@unocss/reset": "^0.51.13",
		"svelte-legos": "^0.2.1"
	},
	"devDependencies": {
		"@iconify-json/mdi": "^1.1.52",
		"@oxygen/eslint-config": "workspace:*",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.16.3",
		"@unocss/eslint-plugin": "^0.51.13",
		"@unocss/extractor-svelte": "^0.51.13",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"unocss": "^0.51.13",
		"vite": "^4.3.7"
	}
}
