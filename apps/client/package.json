		"@oxygen/eslint-config": "workspace:*",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.0",
		"@unocss/eslint-plugin": "^0.52.4",
		"@unocss/extractor-svelte": "^0.52.4",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"unocss": "^0.52.4",
		"vite": "^4.3.9"
	}
