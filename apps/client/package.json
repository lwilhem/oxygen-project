{
	"name": "@oxygen/client",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "vite dev",
		"format": "prettier --write --ignore-path ../../.gitignore \"**/*.{svelte,js,ts,html,json,cjs}\"",
		"lint": "eslint --ignore-path ../../.gitignore \"**/*.{ts,svelte}\"",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"build": "vite build"
	},
	"devDependencies": {
		"@oxygen/eslint-config": "workspace:*",
		"@sveltejs/adapter-vercel": "^2.4.3",
		"@sveltejs/kit": "^1.16.3",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5"
	}
}
